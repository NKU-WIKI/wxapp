{
  "name": "nkuwiki",
  "version": "0.2.0",
  "private": true,
  "description": "南开Wiki校园知识共享平台",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky",
    "commit": "cz",
    "build:weapp": "taro build --type weapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "analyze:weapp": "cross-env ANALYZE=true npm run build:weapp",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "stylelint": "stylelint 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint 'src/**/*.{scss,css}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,md}'",
    "upload": "node scripts/upload.js",
    "preview": "GENERATE_PREVIEW=true node scripts/upload.js"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "aokimi0",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@reduxjs/toolkit": "^2.8.2",
    "@tarojs/components": "4.1.4",
    "@tarojs/helper": "4.1.4",
    "@tarojs/plugin-framework-react": "4.1.4",
    "@tarojs/plugin-platform-weapp": "4.1.4",
    "@tarojs/react": "4.1.4",
    "@tarojs/runtime": "4.1.4",
    "@tarojs/shared": "4.1.4",
    "@tarojs/taro": "4.1.4",
    "@types/classnames": "^2.3.0",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "marked": "^16.2.0",
    "node-fetch": "^2.7.0",
    "react": "^18.0.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "taro-skeleton": "^2.0.4",
    "taro-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tarojs/cli": "4.1.4",
    "@tarojs/plugin-generator": "4.1.4",
    "@tarojs/plugin-html": "^4.1.4",
    "@tarojs/webpack5-runner": "4.1.4",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.19.4",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "4.1.4",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-taro": "4.1.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "file-loader": "^6.2.0",
    "husky": "^9.1.7",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "imagemin": "^9.0.1",
    "lint-staged": "^16.1.2",
    "miniprogram-ci": "^2.1.26",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "sass": "^1.75.0",
    "sharp": "^0.34.3",
    "stylelint": "^16.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "terser-webpack-plugin": "^5.3.14",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
