/* 发现页面 - 开发中 */
.container {
  min-height: 100vh;
  background-color: #f7f7f7;
}

/* 开发中提示样式 */
.developing-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 30% 0;
  width: 100%;
}

.developing-text {
  font-size: 18px;
  color: #666;
  margin-top: 20px;
  font-weight: 500;
}

.developing-subtext {
  font-size: 14px;
  color: #999;
  margin-top: 10px;
}